{
  "name": "ember-theemo",
  "version": "0.1.0",
  "description": "Integrates theemo themes into the ember build",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/theemo-tokens/ember-theemo",
  "license": "MIT",
  "author": "gossi",
  "files": [
    "addon-main.js",
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs",
    "lint": "concurrently 'npm:lint:*(!fix)'",
    "lint:fix": "concurrently 'npm:lint:*:fix'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "start": "rollup --config --bundleConfigAsCjs --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^0.1.0",
    "broccoli-debug": "^0.1.0",
    "broccoli-file-creator": "^0.1.0",
    "broccoli-funnel": "^0.1.0",
    "broccoli-merge-trees": "^0.1.0",
    "ember-get-config": "^0.1.0"
  },
  "peerDependencies": {
    "@glimmer/tracking": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^0.1.0",
    "@babel/plugin-proposal-class-properties": "^0.1.0",
    "@babel/plugin-proposal-decorators": "^0.1.0",
    "@babel/plugin-syntax-decorators": "^0.1.0",
    "@babel/plugin-transform-runtime": "^0.1.0",
    "@babel/plugin-transform-typescript": "^0.1.0",
    "@babel/preset-typescript": "^0.1.0",
    "@embroider/addon-dev": "^0.1.0",
    "@glimmer/component": "^0.1.0",
    "@glimmer/tracking": "^0.1.0",
    "@gossi/config-eslint": "^0.1.0",
    "@gossi/config-prettier": "^0.1.0",
    "@gossi/config-targets": "^0.1.0",
    "@rollup/plugin-babel": "^0.1.0",
    "concurrently": "^0.1.0",
    "ember-source": "^0.1.0",
    "eslint": "^0.1.0",
    "prettier": "^0.1.0",
    "rollup": "^0.1.0",
    "rollup-plugin-ts": "^0.1.0",
    "typescript": "^0.1.0",
    "webpack": "^0.1.0"
  },
  "engines": {
    "node": ">= 16.*"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./test-support": "./dist/test-support/index.js",
    "./addon-main.js": "./addon-main.js"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./services/theemo.js": "./dist/_app_/services/theemo.js"
    }
  },
  "volta": {
    "extends": "../package.json"
  }
}
