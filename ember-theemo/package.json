{
  "name": "ember-theemo",
  "version": "1.0.1",
  "description": "Integrates theemo themes into the ember build",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/theemo-tokens/ember-theemo",
  "license": "MIT",
  "author": "gossi",
  "files": [
    "addon-main.cjs",
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "rollup --config --bundleConfigAsCjs",
    "lint": "pnpm run '/^lint:(?!.*(fix)).*$/'",
    "lint:fix": "pnpm run '/^lint:[^:]*:fix/'",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "tsc --noEmit",
    "prepublishOnly": "pnpm build",
    "start": "rollup --config --bundleConfigAsCjs --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.10.0",
    "@theemo/theme": "^1.0.1",
    "broccoli-debug": "^0.6.5",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "ember-get-config": "^2.1.1",
    "unplugin": "^2.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-syntax-decorators": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@ember/library-tsconfig": "^1.1.3",
    "@embroider/addon-dev": "^7.1.5",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@gossi/config-eslint": "^1.1.2",
    "@gossi/config-prettier": "^1.0.1",
    "@gossi/config-targets": "^1.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@types/node": "^18.19.119",
    "ember-source": "^6.5.0",
    "eslint": "^9.30.1",
    "eslint-plugin-ember": "^12.5.0",
    "prettier": "^3.6.2",
    "rollup": "^4.45.1",
    "rollup-plugin-multi-input": "^1.5.0",
    "rollup-plugin-ts": "^3.4.5",
    "type-fest": "^3.13.1",
    "typescript": "^5.8.3",
    "webpack": "^5.100.2"
  },
  "engines": {
    "node": "^20.11.0 || >=22"
  },
  "exports": {
    ".": "./dist/index.js",
    "./addon-main.cjs": "./addon-main.cjs",
    "./lib": {
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.cjs"
    },
    "./lib/*": {
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.cjs"
    },
    "./*": "./dist/*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./services/theemo.js": "./dist/_app_/services/theemo.js"
    }
  }
}
