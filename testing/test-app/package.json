{
  "name": "test-app",
  "version": "0.1.0",
  "description": "Integrates theemo themes into the ember build",
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm:dev:*'",
    "dev:package": "pnpm run --filter='ember-theemo' start",
    "dev:test-app": "ember serve -e test -p 4300",
