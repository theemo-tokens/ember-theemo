{
  "name": "ember-theemo",
  "version": "0.0.5",
  "description": "Integrates theemo themes into the ember build",
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^4.2.1",
    "ember-get-config": "^0.3.0"
  },
  "devDependencies": {
    "@clark/eslint-config-ember": "^2.2.0",
    "@clark/eslint-config-ember-typescript": "^2.2.0",
    "@clark/eslint-config-node": "^2.2.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.8",
    "@embroider/test-setup": "^0.42.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.16.5",
    "@types/ember-qunit": "^3.4.13",
    "@types/ember__test-helpers": "^2.0.0",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-debug": "^0.6.5",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "4.2.0",
    "ember-auto-import": "^1.11.3",
    "ember-cli": "~3.26.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-get-config": "^0.3.0",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.27.5",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.29.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1"
  },
  "engines": {
    "node": ">= 14.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "volta": {
    "node": "14.17.1",
    "yarn": "1.22.5"
  }
}
